{
    "name": "webexToLink",
    "version": "1.1",
    "description": "Ottieni il link delle registrazioni webex",
    "manifest_version": 2,
    "permissions": [
        "activeTab",
        "webRequest",
        "clipboardWrite",
        "https://*/*",
        "http://*/*"
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "browser_action": {
        "default_icon": "icon.png"
    }
}